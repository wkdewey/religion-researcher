

<%= form_for project do |f| %>
  <%= f.label :title, value: "Project name" %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :texts %>
  <br>
  <%= f.collection_check_boxes :text_ids, texts, :id, :title %>
  <br>
  <%= f.submit %>
<% end %>