<%= render partial: "details", locals: { text: @text } %>
<p>Description: <%= @text.subject %></p>
<h2>notes</h2>
<% @text.notes.each do |note| %>
  <% if note.content %>
    <h3>From the project <%= note.project.title %> </h3>
    <%= note.content  %>
  <% end %>
<% end %>
<%= link_to "Edit text", edit_text_path(@text) %>